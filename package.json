{
  "name": "leaguanalytics",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "client": "cd client && react-scripts-ts start",
    "server": "ts-node -O '{\"module\": \"commonjs\"}' index.ts",
    "start:concurrent": "concurrently \"yarn server\" \"yarn client\"",
    "start": "yarn server",
    "heroku-postbuild": "cd client && yarn && yarn --only=dev --no-shrinkwrap && yarn build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "proxy": "http://localhost:3001/",
  "dependencies": {
    "concurrently": "^3.6.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "kayn": "^0.8.15",
    "node-fetch": "^2.1.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "redis": "^2.8.0",
    "ts-node": "^7.0.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0"
  },
  "devDependencies": {
    "@material-ui/core": "^1.2.3",
    "@types/dotenv": "^4.0.3",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.4",
    "@types/node-fetch": "^2.1.1",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "typescript": "^2.9.2"
  }
}
